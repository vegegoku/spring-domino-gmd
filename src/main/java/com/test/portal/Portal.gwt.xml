<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
  "http://gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="Portal">
  <stylesheet src="/portal.css"/>

  <!-- Inherit the core Web Toolkit stuff.                  -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.progressoft.brix.domino.gwt.Core"/>
  <inherits name="com.progressoft.brix.domino.api.Domino"/>
  <inherits name="com.progressoft.brix.domino.logger.Logging"/>
  <inherits name="org.fusesource.restygwt.RestyGWT"/>
  <inherits name="elemental2.core.Core"/>
  <inherits name="elemental2.dom.Dom"/>

  <!-- Specify the app entry point class.                   -->
  <entry-point class='com.test.portal.client.Portal'/>

  <!-- other inherited modules, such as com.google.gwt.user.User -->
  <inherits name="com.google.gwt.http.HTTP"/>

  <extend-configuration-property name="document.compatMode" value="BackCompat"/>

  <!--Specify the paths for translatable code-->
  <source path="client"/>
</module>